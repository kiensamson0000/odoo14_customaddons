<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Cron Module Haravan Integration -->
        <!--        -->
        <record id="api_haravan_categories_cron" model="ir.cron">
            <field name="name">####API Categories</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_haravan_categories"/>
            <field name="state">code</field>
            <field name="code">model.get_categories_haravan()</field>
        </record>

